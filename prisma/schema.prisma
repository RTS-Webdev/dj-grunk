generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int     @id @default(autoincrement())
  name      String
  email     String  @unique
  pass  String
  grunker   Int      @default(1500)
  createdAt DateTime @default(now())
}